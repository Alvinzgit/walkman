pipeline {
	agent any
	
	stages {
		stage('Build') {
			steps {
				sh 'chmod +x gradlew'
                sh './gradlew build --build-cache --info'
				}
		}
		stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
		stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
	}
}